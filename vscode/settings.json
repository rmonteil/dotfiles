{
  "editor.wordWrap": "on",
  "php.validate.executablePath": "/usr/bin/php7.1",
  "editor.minimap.enabled": false,
  "editor.fontLigatures": true,
  "workbench.iconTheme": "vscode-icons",
  "todohighlight.isCaseSensitive": false,
  "todohighlight.keywordsPattern": "\/\/ *(todo|fix(?:me)?)[^\n\r]*",
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.history",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "local-history.maxDisplay": 100,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.idea": true,
    "**/.git": true,
    "**/.vscode": true,
    "**/.history": true,
    "**/vendor": true,
    "**/cache": true,
    "**/dist": true,
    "**/build": true,
    "**/jest_*": true
  },
  "vsicons.dontShowNewVersionMessage": true,
  "editor.fontFamily": "'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.fontFamily": "'Droid Sans Mono for Powerline', 'Courier New', monospace, 'Droid Sans Fallback'",
  "explorer.autoReveal": false,
  "search.useIgnoreFiles": true,
  "git.autofetch": true,
  "workbench.colorTheme": "One Dark Pro Vivid",
  "editor.tabSize": 4,
  "extensions.ignoreRecommendations": false,
  "editor.formatOnSave": true
}